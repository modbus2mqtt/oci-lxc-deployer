{
  "name": "Mosquitto MQTT",
  "description": "Eclipse Mosquitto MQTT broker for E2E testing",
  "extends": "docker-compose",
  "icon": "icon.svg",
  "parameters": [
    {
      "id": "hostname",
      "name": "Hostname",
      "type": "string",
      "required": false,
      "description": "Hostname for the Docker Compose container. Required when creating an application from the docker-compose framework.",
      "template": "application.json",
      "default": "mosquitto-e2e-test"
    },
    {
      "id": "compose_file",
      "name": "Docker Compose File",
      "type": "string",
      "upload": true,
      "required": true,
      "description": "Docker Compose YAML file (upload). Upload this file first to extract service information.",
      "template": "application.json",
      "default": ""
    }
  ],
  "properties": [
    {
      "id": "compose_file",
      "value": ""
    },
    {
      "id": "ostype",
      "value": "alpine"
    }
  ],
  "installation": {}
}